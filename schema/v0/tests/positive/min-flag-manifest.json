{
  "$schema": "../../flag-manifest.json",
  "flags": {
    "booleanFlag": {
      "flagType": "boolean",
      "defaultValue": true
    },
    "stringFlag": {
      "flagType": "string",
      "defaultValue": "default"
    },
    "integerFlag": {
      "flagType": "integer",
      "defaultValue": 50
    },
    "floatFlag": {
      "flagType": "float",
      "defaultValue": 0.15
    },
    "objectFlag": {
      "flagType": "object",
      "defaultValue": {
        "primaryColor": "#007bff",
        "secondaryColor": "#6c757d"
      }
    }
  }
}